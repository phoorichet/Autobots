<h1>Listing rncs</h1>

<table class="table table-hover table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Sgsn</th>
      <th>Sgsn name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @rncs.each do |rnc| %>
      <tr>
        <td><%= rnc.name %></td>
        <td><%= rnc.sgsn_id %></td>
        <td><%= rnc.sgsn.name %></td>
        <td><%= link_to 'Show', rnc %></td>
        <td><%= link_to 'Edit', edit_rnc_path(rnc) %></td>
        <td><%= link_to 'Destroy', rnc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rnc', new_rnc_path %>
